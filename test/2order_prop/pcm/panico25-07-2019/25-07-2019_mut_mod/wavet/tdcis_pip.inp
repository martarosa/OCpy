&general n_ci=15,n_ci_read=15
mol_cc= 0. 0. 0.
medium='sol'
n_f=1 
dt=0.01
n_step=25000
n_out=1
propa='n'
/ 

&field 
Ffld='pip'
omega(1)=0.3239892104044628
sigma(1)=125,
t_mid=125
radiative='non'
npulse=1
fmax(1,1)= 0.5 
fmax(2,1)= 0.0 
fmax(3,1)= 0.0
/

&sse dissipative='non'
/

&spectra start=80, 
tau= 2000.0 2000.0
dir_ft= 1. 1. 1. 
/

&pop_coh
tar = 'all'
all_pop= 'yes'
all_coh='yes'
write_bin='n'
/


! Here starts the tdplas part
!
&PROPAGATE  
propagation_type='ief'
interaction_type='pcm'
interaction_stride=1
interaction_init='non-scf'
!test_type='s-l' 
local_field='non'
/

&MEDIUM
medium_type='sol'
medium_init='fro'
bem_type='diag'
bem_read_write='rea'
!print_lf_matrix='non'
/

&SURFACE
input_surface='fil'
/

&EPS_FUNCTION
epsilon_omega='deb'
eps_0=35.84
eps_d=1.803   
!tau_deb=500. 
tau_deb=139320.
/


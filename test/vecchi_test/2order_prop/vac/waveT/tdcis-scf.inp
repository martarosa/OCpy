&general 
n_ci=15 
n_ci_read=15  
mol_cc= 0. 0. 0.
medium='vac'
n_f=1
dt=0.20
n_step=10
n_out=1
/ 

&field 
Ffld='snd'
omega(1)=0.26
sigma(1)=10,
t_mid=5000
radiative='non'
npulse=1
fmax(1,1)= 0. 
fmax(1,2)=0. 
fmax(1,3)=0.      
/

&sse
dissipative='non'
/

&spectra 
start=80 
tau= 2000. 2000.
dir_ft= 1. 1. 1. 
/

&pop_coh
tar = 'all'
all_pop= 'yes'
all_coh='yes'
write_bin='n'
/


! Here starts the tdplas part
!
&PROPAGATE  
propagation_type='ief'
interaction_type='pcm'
interaction_stride=1
interaction_init='SCF_ES'
scf_threshold=14
debug_type='non' 
local_field='non'
/

&MEDIUM
medium_type='vac'
medium_init='vac'
bem_type='diag'
bem_read_write='rea'
!print_lf_matrix='non'
/

&SURFACE
input_surface='fil'
/

&EPS_FUNCTION
epsilon_omega='drl'
eps_0=1000.
eps_d=1. 
eps_A=0.110224   
eps_gm=0.000757576
eps_w0=0.  
f_vel=0. 
/

